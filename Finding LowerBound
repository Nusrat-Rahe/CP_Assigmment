#include<bits/stdc++.h>
using namespace std;

int lowerBound(vector<int>&arr,int target) {
    int n=arr.size();
    for (int i=0; i<n;i++) {
        if (arr[i]>=target) {
            return i;
        }
    }
return n;
}

int main() {
    int n, target;
    cin >> n;
    vector<int>arr(n);
    for (int i=0; i<n;i++) {
        cin>>arr[i];
    }
    cin >> target;
    cout<<lowerBound(arr, target)<< endl;
    return 0;
}
