#include <bits/stdc++.h>
using namespace std;

int lowerBound(vector<int>& arr, int target) {
    int left = 0, right = arr.size();
    while (left < right) {
        int mid = left + (right-left)/2;
        if (arr[mid] < target)
            left = mid + 1;
        else
            right = mid;
    }
    return left;
}

int main() {
    int n, target;
    cin >> n;
    vector<int> arr(n);
    for (int i = 0; i < n; i++) cin >> arr[i];
    cin >> target;
    
    cout << lowerBound(arr, target) << endl;
    return 0;
}

##using stl
#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<int> arr(n);
    for(int i = 0; i < n; i++){
        cin >> arr[i];
    }

    int X;
    cin >> X;
    if(binary_search(arr.begin(), arr.end(), X)) {
        int index = lower_bound(arr.begin(), arr.end(), X) - arr.begin();
        cout << index << endl;
    } else {
        cout << -1 << endl;
    }

    return 0;
}