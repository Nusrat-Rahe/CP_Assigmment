#include <bits/stdc++.h>
using namespace std;

int main(){
    int n, target;
    cin >> n;
    vector<int>arr(n);
    for (int i=0;i<n;i++)
        cin>>arr[i];
    cin>>target;
    auto first= lower_bound(arr.begin(),arr.end(),target);
    auto last= upper_bound(arr.begin(),arr.end(),target);

    if (first==arr.end()||*first!=target){
        cout<<-1<<" "<<-1<<endl;
    } else {
        cout<<first-arr.begin()<< " " <<(last-arr.begin()-1)<<endl;
    }
    return 0;
}
