#include <bits/stdc++.h>
using namespace std;

int binarysearchX(int arr[],int n,int X){
    int low=0,high=n-1;
    while (low <= high){
        int mid=low+(high - low)/2;
        if (arr[mid] == X)
            return mid;
        else if(arr[mid]<X)
            low=mid+1;
        else
            high=mid-1;
    }
    return -1;
}
int main() {
    int n;
    cin >> n;
    int arr[n];
    for (int i=0; i<n; i++){
         cin >> arr[i];
    }
    int X;
    cin >> X;
    cout << binarysearchX(arr, n, X) << endl;
    return 0;
}



##using stl
#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<int> arr(n);
    for(int i = 0; i < n; i++){
        cin >> arr[i];
    }

    int X;
    cin >> X;
    if(binary_search(arr.begin(), arr.end(), X)) {
        int index = lower_bound(arr.begin(), arr.end(), X) - arr.begin();
        cout << index << endl;
    } else {
        cout << -1 << endl;
    }

    return 0;
}